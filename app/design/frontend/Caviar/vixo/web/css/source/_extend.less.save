
@media (min-width: 769px), print {
  .page-header .panel.wrapper {
    border-bottom: 1px solid #e8e8e8;
}
  .panel.header {
    padding: 0px !important;
  }
   .header.content {
    padding: 0px !important;
  }
}
@media screen and (min-width: 768px) {
    @media (min-width: 769px), print {
        .navigation,
        .breadcrumbs,
       /* .page-header .header.panel, */ // intentionally removed
        .header.content,
         /* .footer.content, */
        .page-wrapper > .widget,
        .page-wrapper > .page-bottom,
        .block.category.event,
        .top-container,
        .page-main {
            box-sizing: border-box;
            margin-left: auto;
            margin-right: auto;
            max-width: 1280px;
            padding-left: 20px;
            padding-right: 20px;
            width: auto;
        }

        /* Override to make header full width */
        .page-header .header.panel {
            max-width: 100%;
            margin: 0;
            padding: 0 20px; // optional, adjust if needed
        }
        .footer.content{
           max-width: 100%;
            margin: 0;
            padding: 0 20px; // optional, adjust if needed
        }
    }
}

